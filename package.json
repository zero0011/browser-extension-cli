{
  "name": "browser-extension-cli",
  "version": "0.0.1",
  "description": "The CLI support you devlop Broswer Extension by React and Vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/zero0011/browser-extension-cli"
  },
  "main": "index.js",
